@model Geo4Students.ViewModels.JaarViewModel

@{
    ViewBag.Title = "Start";
}

<div id="jaarSelect">
    @using (Html.BeginForm(null, null, FormMethod.Post, new {id = "testForm"}))
    {
        <div class="form-group">
            @Html.DropDownListFor(model => model.SelectedJaar, new SelectList(Model.SubjectList), "<kies leerjaar>", new {@class = "form-control"})
        </div>
        <div class="form-group">
            <input type="submit" value="Bevestig" id="button"/>
        </div>
    }
</div>

<script src="/Scripts/jquery.js"></script>
<script src="/Scripts/HomeIndex.js"></script>